     <string notr="true">/* Set the background of the QTabWidget to black */
QTabWidget {
    background-color: black;
}

/* Set the color of the tab text to white */
QTabWidget::pane {
    color: white;
}

/* Set the background of the tab bar to transparent */
QTabWidget::tab-bar {
    background-color: transparent;
}

/* Set the color of the tab indicator to white */
QTabWidget::tab-bar::selected {
    color: white;
}

/* Create a light ray/matrix effect for the tab bar */
QTabBar {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 transparent,
        stop:0.3 rgba(255, 255, 255, 0.1),
        stop:0.4 rgba(255, 255, 255, 0.9),
        stop:0.6 rgba(255, 255, 255, 0.9),
        stop:0.7 rgba(255, 255, 255, 0.1),
        stop:1 transparent);
    border: none;
    height: 40px;
    padding: 5px;
}

/* Animate the light ray/matrix effect */
QTabBar::indicator {
    background: white;
    border-radius: 5px;
    width: 40px;
    height: 40px;
    position: absolute;
    left: 0;
    top: 0;
    animation: light-ray-matrix 2s linear infinite;
}

/* Define the animation keyframes for the light ray/matrix effect */
@keyframes light-ray-matrix {
    0% {
        left: 0;
    }
    25% {
        left: 60px;
        height: 30px;
        width: 10px;
    }
    50% {
        left: 120px;
        height: 20px;
        width: 20px;
    }
    75% {
        left: 180px;
        height: 30px;
        width: 10px;
    }
    100% {
        left: 240px;
    }
}
